CC		=	icc
CXX		=	icpc
LINKCC		=	icc
LINKCXX		=	icpc
FLAGS		+=	-DNDEBUG -DSCIP_ROUNDING_FE
OFLAGS		+=	-O2 -fp-model precise -w1 -ip -mtune=native
CFLAGS		+=	-std=c99 -D_XOPEN_SOURCE=600
ARFLAGS		=	crs
LDFLAGS		+=	-lirc -lpthread -lm -lstdc++ -shared-intel
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
ifeq ($(SHARED),true)
FLAGS		+=	-fPIC
endif
